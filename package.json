{
  "name": "ng5-ionic3-starter",
  "version": "1.0.0",
  "author": "haokur",
  "homepage": "https://github.com/haokur/ng5-ionic3-starter",
  "private": true,
  "scripts": {
    "start": "ionic-app-scripts serve NODE_ENV=development",
    "build": "bnr build",
    "clean": "ionic-app-scripts clean",
    "lint": "ionic-app-scripts lint"
  },
  "betterScripts": {
    "dev": {
      "command": "ionic-app-scripts serve",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "build": {
      "command": "ionic-app-scripts build --prod",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "@angular/common": "5.0.0",
    "@angular/compiler": "5.0.0",
    "@angular/compiler-cli": "5.0.0",
    "@angular/core": "5.0.0",
    "@angular/forms": "5.0.0",
    "@angular/http": "5.0.0",
    "@angular/platform-browser": "5.0.0",
    "@angular/platform-browser-dynamic": "5.0.0",
    "@ionic-native/core": "4.3.2",
    "@ionic-native/splash-screen": "4.3.2",
    "@ionic-native/status-bar": "4.3.2",
    "@ionic/storage": "2.1.3",
    "@ngrx/store": "5.2.0",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "jsonp": "0.2.1",
    "rxjs": "5.5.2",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.9",
    "better-npm-run": "0.1.0",
    "typescript": "2.4.2"
  },
  "description": "基于 angualr5 + ionic3 + cordova 项目初始化"
}