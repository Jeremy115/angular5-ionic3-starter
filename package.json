{
  "name": "ng5-ionic3-starter",
  "version": "1.0.0",
  "author": "haokur",
  "homepage": "https://github.com/haokur/ng5-ionic3-starter",
  "private": true,
  "scripts": {
    "start": "bnr dev",
    "build": "bnr build",
    "android": "python ./appbuild.py android",
    "android:skip": "python ./appbuild.py android --skipweb",
    "ios": "python ./appbuild.py ios",
    "ios:skip": "python ./appbuild.py ios --skipweb",
    "clean": "ionic-app-scripts clean",
    "lint": "ionic-app-scripts lint"
  },
  "betterScripts": {
    "dev": {
      "command": "ionic-app-scripts serve",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "ionic-app-scripts build",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "@angular/common": "5.0.0",
    "@angular/compiler": "5.0.0",
    "@angular/compiler-cli": "5.0.0",
    "@angular/core": "5.0.0",
    "@angular/forms": "5.0.0",
    "@angular/http": "5.0.0",
    "@angular/platform-browser": "5.0.0",
    "@angular/platform-browser-dynamic": "5.0.0",
    "@ionic-native/camera": "4.7.0",
    "@ionic-native/core": "4.3.2",
    "@ionic-native/crop": "4.7.0",
    "@ionic-native/file-transfer": "4.7.0",
    "@ionic-native/image-picker": "4.7.0",
    "@ionic-native/splash-screen": "4.3.2",
    "@ionic-native/status-bar": "4.3.2",
    "@ionic/storage": "2.1.3",
    "@ngrx/store": "5.2.0",
    "axios": "0.18.0",
    "axios-mock-adapter": "1.15.0",
    "com.synconset.imagepicker": "2.1.10",
    "cordova-android": "6.3.0",
    "cordova-ios": "4.5.4",
    "cordova-plugin-camera": "4.0.3",
    "cordova-plugin-crop": "0.4.0",
    "cordova-plugin-device": "1.1.4",
    "cordova-plugin-file": "6.0.1",
    "cordova-plugin-file-transfer": "1.7.1",
    "cordova-plugin-ionic-webview": "1.2.1",
    "cordova-plugin-splashscreen": "4.0.3",
    "cordova-plugin-telerik-imagepicker": "2.1.10",
    "cordova-plugin-whitelist": "1.3.1",
    "image-picker": "0.3.1",
    "ionic-angular": "3.9.2",
    "ionic-plugin-keyboard": "2.2.1",
    "ionicons": "3.0.0",
    "jsonp": "0.2.1",
    "mockjs": "1.0.1-beta3",
    "rxjs": "5.5.2",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.9",
    "better-npm-run": "0.1.0",
    "typescript": "2.4.2"
  },
  "description": "基于 angualr5 + ionic3 + cordova 项目初始化",
  "cordova": {
    "plugins": {
      "com.synconset.imagepicker": {
        "PHOTO_LIBRARY_USAGE_DESCRIPTION": "允许App访问你的相册?"
      },
      "cordova-plugin-file-transfer": {},
      "cordova-plugin-crop": {},
      "cordova-plugin-camera": {},
      "ionic-plugin-keyboard": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}